{
  "env": {
    "browser": true
  },
  "parser": "babel-eslint",
  "plugins": [
    "react"
  ],
  "rules": {
    "no-console": 0,
    "no-unused-vars": 0,
    "no-multiple-empty-lines": [1, {
      "max": 3,
      "maxBOF": 0,
      "maxEOF": 0
    }],
    "no-param-reassign": ["error", { "props": false }],
    "comma-dangle": ["error", "never"],
    "brace-style": ["error", "stroustrup"],
    "spaced-comment": 0,
    "react/prop-types": 0
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "airbnb-base"]
}